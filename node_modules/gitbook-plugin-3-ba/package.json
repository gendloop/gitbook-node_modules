{
    "name": "gitbook-plugin-3-ba",
    "description": "Baidu Analytics tracking for your gitbook version 3",
    "main": "index.js",
    "version": "0.9.0",
    "engines": {
        "gitbook": ">=3.0.0"
    },
    "homepage": "https://github.com/frankfang/gitbook3-plugin-ba",
    "repository": {
        "type": "git",
        "url": "https://github.com/FrankFang/gitbook3-plugin-ba.git"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/FrankFang/gitbook3-plugin-ba/issues"
    },
    "gitbook": {
        "properties": {
            "token": {
                "type": "string",
                "title": "Baidu Analytics Token",
                "required": true
            },
            "configuration": {
                "type": [ "string", "object" ],
                "title": "Configuration mode",
                "default": "auto"
            }
        }
    }
}
